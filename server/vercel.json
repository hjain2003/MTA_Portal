{
    "version": 2,
    "name": "music-album",
    "builds": [
        {
            "src": "main.jsx",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "^/user(/.*)?$",
            "dest": "https://mta-backend.vercel.app"
        },
        {
            "src": "^/announcement(/.*)?$",
            "dest": "https://mta-backend.vercel.app"
        },
        {
            "src": "^/deadline(/.*)?$",
            "dest": "https://mta-backend.vercel.app"
        },
        {
            "src": "^/resource(/.*)?$",
            "dest": "https://mta-backend.vercel.app"
        },
        {
            "src": "^/exam(/.*)?$",
            "dest": "https://mta-backend.vercel.app"
        },
        {
            "src": "/(.*)",
            "dest": "/main.jsx"
        }
    ]
}
